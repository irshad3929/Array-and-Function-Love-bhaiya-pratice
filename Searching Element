#include <iostream>

using namespace std;
 void Search(int arr[], int size, int key)
 {
     int flag=0;
     for(int i=0;i<size;i++)
     {
         if(arr[i]==key)
         flag=1;
     }
     if(flag==1)
      cout<<"key found "<<endl;
     else
      cout<<"Key not found";
 }
 int main()
 {
     //int flag=0;  not written this type 
     int arr[]={10,4,6,3,7,1};
     Search(arr,6,3);
      return 0;
 }
