
#include <iostream>

using namespace std;
void Intersection(int arr1[],int arr2[], int size)
{
    for(int i=0;i<size;i++)
    {
        for(int j=0;j<size;j++)
        {
            if(arr1[i]==arr2[j])
            cout<<arr1[i]<<" ";
        }
    }
    
}
int main()
{
   int arr1[]={1,3,5,6,9,4};
   int arr2[]={1,7,6,5,4,2};
    Intersection(arr1,arr2,6);
    
    
    

    return 0;
}
